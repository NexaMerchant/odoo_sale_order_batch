<odoo>
    <data>
        <record id="view_batch_print_label_wizard_form" model="ir.ui.view">
            <field name="name">batch.print.label.wizard.form</field>
            <field name="model">batch.print.label.wizard</field>
            <field name="arch" type="xml">
                <form string="确认批量打印面单">
                    <group>
                        <field name="message" nolabel="1"/>
                    </group>
                    <footer>
                        <button name="action_confirm_print" string="打印" type="object" class="btn-primary"/>
                        <button string="取消" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>